{
  "name": "alexandria",
  "version": "0.1.10",
  "description": "A modern desktop app for searching and downloading books.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run clean && webpack",
    "watch": "webpack --watch",
    "dist": "electron-builder",
    "clean": "rm -rf dist node_modules/.cache/webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "analyze": "webpack --analyze"
  },
  "keywords": [],
  "author": {
    "name": "Joshua Miles",
    "email": "joshmilesproduction@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@codaworks/react-glow": "^1.0.6",
    "@fontsource/inter": "^5.0.18",
    "@electron/universal": "^1.4.2",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "clsx": "^2.1.1",
    "electron-dl": "^4.0.0",
    "electron-log": "^5.4.1",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.6.2",
    "framer-motion": "^12.19.2",
    "got": "^14.4.7",
    "re-resizable": "^6.11.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-lazy-load-image-component": "^1.6.3",
    "react-spring": "^10.0.1",
    "socks-proxy-agent": "^8.0.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
