    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.4.0",
    "typescript": "^5.4.5",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^6.0.1"
  },
  "build": {
    "appId": "com.example.alexandria",
    "productName": "Alexandria",
    "files": [
      "dist/",
      "main.js",
      "preload.js"
    ],
    "mac": {
      "target": "dmg",
      "arch": [
          "universal"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "publish": {
      "provider": "github",
      "owner": "joshmiles",
      "repo": "alexandria"
    },
    "electronUniversal": {
      "allowList": [
        "dist/mac-universal-x64-temp/Electron.app"
      ]
  }
  }
